@{
    CaseLogModel pCaseLogs = null;
    var currentCaseId = Model.CasePreview.Id;
    var helper = ManualDependencyResolver.Get<ICommonFunctions>();
    if (currentCaseId != null)
    {
        pCaseLogs = helper.GetCaseLogs(currentCaseId);
    }
    var appType = ConfigurationManager.AppSettings[AppSettingsKey.CurrentApplicationType].ToString().ToLower();
}

<script type="text/javascript">
    window.appParameters = {
        uploadLogFileUrl: '@Url.Action("UploadLogFile", "Case")',
        logFileKey: '@Model.LogFileGuid',
        getLogFilesUrl: '@Url.Action("GetLogFiles", "Case")',
        fileAlreadyExistsMsg: '@Html.Raw(Translation.Get("File already exists", Enums.TranslationSource.TextTranslation))',
        deleteLogFileUrl: '@Url.Action("DeleteLogFile")',
        downloadLogFileUrl: '@Url.Action("DownloadLogFile")',
        downloadLogFileParamUrl: '@(!string.IsNullOrEmpty(Model.LogFileGuid) ? "id=" + Model.LogFileGuid + "&" : string.Empty)',
        saveLogMessageUrl: '@Url.Content("~/Case/_CaseLogNote")',
        casePreviewId: '@Model.CasePreview.Id'
    };
</script>

