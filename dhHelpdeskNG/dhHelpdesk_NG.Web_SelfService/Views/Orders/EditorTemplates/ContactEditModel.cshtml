
@model DH.Helpdesk.SelfService.Models.Orders.OrderEdit.ContactEditModel

<h4>@Translation.Get("Kontakt")</h4>
<div class="container">
    <div class="row form-horizontal">
        <div class="col-md-12 dh-col ">

            @Html.EditorFor(m => m.Id, "Fields/Text", new { inputAttributes = new Dictionary<string, object> { { "maxlength", "200" } } })
            @Html.EditorFor(m => m.Name, "Fields/Text", new { inputAttributes = new Dictionary<string, object> { { "maxlength", "50" } } })
            @Html.EditorFor(m => m.Phone, "Fields/Text", new { inputAttributes = new Dictionary<string, object> { { "maxlength", "50" } } })
            @Html.EditorFor(m => m.Email, "Fields/Text", new { inputAttributes = new Dictionary<string, object> { { "maxlength", "50" } } })
        </div>
    </div>
</div>