@model DH.Helpdesk.EForm.FormLib.Models.FormModel
@using DH.Helpdesk.EForm.FormLib;
@using DH.Helpdesk.EForm.FormLib.Models;

<div class="tab-pane" id="SSCUseOnly">
    <div class="span6">
        <table class="table">
            <tbody>
                <tr>
                    <td class="tdstrong">@Model.Label("NewCo-WorkerID")</td>
                    <td>
                        @Model.Element("NewCo-WorkerID")
                    </td>
                </tr>

            </tbody>
        </table>
    </div>
</div>
<script>
    $('#NewCo-WorkerID').on('change', function () {
        $('#Co-WorkerID').val($('#NewCo-WorkerID').val());
        $('#hidden_Co-WorkerID').val($('#NewCo-WorkerID').val());

    }); 
</script>
   