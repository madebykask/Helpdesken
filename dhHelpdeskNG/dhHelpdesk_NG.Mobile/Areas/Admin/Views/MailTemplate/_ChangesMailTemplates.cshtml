@using DH.Helpdesk.Mobile.Infrastructure
@model DH.Helpdesk.Mobile.Areas.Admin.Models.MailTemplateIndexViewModel
<table class="table table-striped table-bordered table-hover">
    @foreach (var item in Model.Changes)
    {
        <tr><td><a href="@Url.Action("edit", "mailtemplate", new { area = "admin", id = item.Value, customerId = Model.Customer.Id, languageId = 2 })"><strong>@Translation.Get("E-post mall", Enums.TranslationSource.TextTranslation):</strong> @Html.DisplayFor(modelItem => item.Text)</a>
        </td></tr>
    }
</table>