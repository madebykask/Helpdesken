@model DH.Helpdesk.Web.Models.Inventory.EditModel.ComputerModuleEditModel
@using (Html.BeginForm("NewComputerModule", "Inventory", FormMethod.Post, new { id = "computerModule_edit_form" }))
{
    <div class="secnavs fixed">
        <div class="pull-left">
            <ul class="secnav">
                <li id="menu1">
                    <input type="submit" value="@Translation.Get("Spara")" class="btn" onclick=" $('#computerModule_edit_form').submit(); "/>
                </li>
                <li id="menu2">@Html.ActionLink(Translation.Get("Avbryt"), "Index", null, new { @class = "btn" })</li>
            </ul>
        </div>
    </div>

    <div class="content">
        <div class="container">
            <ul class="nav nav-tabs" id="inventory_tab">
                <li class="active">
                    <a href="#fragment-1">@Translation.Get(Model.ModuleType.ToString())</a>
                </li>
            </ul>
            <div class="tab-content">
                <div class="tab-pane tab-pane-border active" id="fragment-1">
                    @Html.EditorFor(m => m.Name)
                    @Html.HiddenFor(m => m.ModuleType)
                </div>
            </div>
        </div>
    </div>
}


