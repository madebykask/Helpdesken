@model DH.Helpdesk.Web.Areas.Admin.Models.CustomerInputViewModel
@if (TempData["Error"] != null)
{
    <div class="alert alert-error">@Translation.Get("Kund")
        @Translation.Get("som används går inte att ta bort")
    </div>    
}
<div class="content">
    <div class="container">
        <ul class="nav nav-tabs" id="myTab">
            <li class="active"><a href="#subfragment-1">@Translation.Get("Kund") - @Model.Customer.Name</a></li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane tab-pane-border active" id="subfragment-1">
                <div class="container">
                    @Html.Partial("_Customer", Model)
                </div>
            </div>
        </div>
    </div>
    @Html.HiddenFor(model => model.Setting.Id)
    <input type="hidden" name="activeTab" id="activeTab" value="" />
</div>

