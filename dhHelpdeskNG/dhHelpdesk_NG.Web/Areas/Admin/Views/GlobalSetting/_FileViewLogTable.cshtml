
@using System.Globalization
@using DH.Helpdesk.Common.Constants
@model IList<DH.Helpdesk.Web.Models.FileViewLogs.FileViewLogItemViewModel>

@{ Layout = null; }

@foreach (var item in Model)
{
<tr>
    <td>@item.DepartmentName</td>
    <td>@item.CreatedDate.ToString(DateFormats.DateTime, CultureInfo.InvariantCulture)</td>
    <td>@item.UserName</td>
    <td>@item.CaseNumber</td>
    <td>@item.ProductAreaName</td>
    <td>@item.FileName</td>
    <td>@item.FilePath</td>
    <td>@item.Operation</td>
</tr>
}

