@model dhHelpdesk_NG.Web.Areas.Admin.Models.GlobalSettingWatchDateViewModel
@{
    ViewBag.Title = "New WatchDate";           
}
@using (Html.BeginForm("newwatchdate", "globalsetting", new { id = Model.WatchDateCalendarValue.Id }, FormMethod.Post, new { id = "target" }))
{
    <div class="secnavs">
        <div class="pull-left">
            <ul class="secnav">
                <li>@Html.ActionLink(Translation.Get("Spara", Enums.TranslationSource.TextTranslation), "newwatchdate", null, new { onclick = "submitForm();return false;", @class = "btn" })</li>
                <li>@Html.ActionLink(Translation.Get("Avbryt", Enums.TranslationSource.TextTranslation), "index", null, new { @class = "btn" })</li>
            </ul>
        </div>
    </div>
    @Html.Partial("_WatchDate", Model)

}
