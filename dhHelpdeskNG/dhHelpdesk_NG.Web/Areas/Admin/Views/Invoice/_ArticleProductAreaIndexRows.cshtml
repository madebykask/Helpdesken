@model DH.Helpdesk.Web.Areas.Admin.Models.Invoice.InvoiceArticleProductAreaIndexRowsModel
<div id="ArticleIndexRows">
    <table class="table table-striped table-bordered table-hover" id="tblInviceArticle">
        <thead>
        <tr>
            <th>@Translation.Get("Artikelnamn", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Produktområde", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Ta bort", Enums.TranslationSource.TextTranslation)</th>
        </tr>
        </thead>
        @foreach (var row in Model.Data)
        {
        <tr>
            <td>@row.InvoiceArticleNumber - @row.InvoiceArticleName</td>
            <td>@row.ProductAreaName</td>
            <td><a id="" class="btn bt-small" data-field="" title="@Translation.Get("Ta bort")"><i class="icon-remove"></i></a></td>
        </tr>
        }
    </table>
</div>  
           