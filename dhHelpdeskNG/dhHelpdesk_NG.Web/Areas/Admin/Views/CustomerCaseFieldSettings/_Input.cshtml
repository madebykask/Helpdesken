@helper RenderLockedFieldOption(string name, int value)
{
    <select name="@name">
        @foreach(var option in Model.LockedFieldOptions)
        {
            <option @(value.ToString() == option.Value ? " selected=selected" : "") value="@option.Value">@option.Text</option>
        }
    </select>
}

