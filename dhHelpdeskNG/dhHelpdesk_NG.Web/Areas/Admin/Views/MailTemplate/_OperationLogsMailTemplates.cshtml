@model dhHelpdesk_NG.Web.Areas.Admin.Models.MailTemplateIndexViewModel
<table class="table table-striped table-bordered table-hover">
    @foreach (var item in Model.OperationLogs)
    {
        <tr><td>@Translation.Get("E-post mall: ", Enums.TranslationSource.TextTranslation)<a href="@Url.Action("edit", "mailtemplate", new { area = "admin", id = item.Value, customerId = Model.Customer.Id })">@Html.DisplayFor(modelItem => item.Text)</a>
        </td></tr>
    }
</table>