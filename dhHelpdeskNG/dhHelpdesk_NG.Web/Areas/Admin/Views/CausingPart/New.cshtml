@model DH.Helpdesk.Web.Areas.Admin.Models.CausingPart.CausingPartViewModel

@{
    ViewBag.Title = "New";
}
@using (Html.BeginForm("New", "CausingPart", new { area = "admin" }, FormMethod.Post, new { id = "target" }))
{
    <div class="secnavs fixed">
        <div class="pull-left">
            <ul class="secnav">
                <li>@Html.ActionLink(Translation.Get("Spara"), "New", null, new { onclick = "$('#target').submit();return false;", @class = "btn" })</li>
                <li>@Html.ActionLink(Translation.Get("Avbryt"), "Index", new { customerId = Model.CausingPart.CustomerId }, new { @class = "btn" })</li>
            </ul>
        </div>
    </div>
    @Html.Partial("_Input", Model)
}
<script type="text/javascript">

    $("input:text:visible:first").focus();

</script>
