@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Areas.Licenses.Models.Vendors.VendorsContentModel

<table class="table table-striped table-bordered table-hover table-pointer">
    <thead>
        <tr>
            <th>@Translation.Get("Försäljare")</th>
            <th>@Translation.Get("Kontakt")</th>
            <th>@Translation.Get("Telefon")</th>
            <th>@Translation.Get("E-post")</th>
            <th>@Translation.Get("Hemsida")</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var vendor in Model.Vendors)
        {
            <tr onclick="window.location.href='@Url.Action("Vendor", new RouteValueDictionary { { "productId", vendor.VendorId } })'">
                <td>@vendor.VendorName</td>
                <td>@vendor.VendorContact</td>
                <td>@vendor.VendorPhone</td>
                <td>@vendor.VendorEmail</td>
                <td>@vendor.VendorHomePage</td>
            </tr>
        }        
    </tbody>
</table>