@model DH.Helpdesk.Web.Areas.Orders.Models.OrderFieldSettings.FieldSettings.FullFieldSettingsModel

@using (Html.BeginForm("SaveSettings", "OrderFieldSettings", 
            FormMethod.Post,
            new { id = "SaveSettingsForm" }))
{
    @Html.EditorFor(m => m)
}
<script type="text/javascript">
    $(function() {
        $('.switchcheckbox').bootstrapSwitch('onText', trans_yes);
        $('.switchcheckbox').bootstrapSwitch('offText', trans_no);
        $('.switchcheckbox').bootstrapSwitch('size', 'small');
        $('.switchcheckbox').bootstrapSwitch('onColor', 'success');
    });
</script>