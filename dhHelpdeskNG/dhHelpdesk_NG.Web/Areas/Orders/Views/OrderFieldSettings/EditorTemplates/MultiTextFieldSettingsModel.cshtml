@model DH.Helpdesk.Web.Areas.Orders.Models.OrderFieldSettings.FieldSettings.MultiTextFieldSettingsModel
    
<td>
    @{ Html.RenderPartial("Partials/OrderFieldWebTypes"); }
</td>
<td>@Html.LabelFor(m => m)</td>
<td>@Html.CheckBoxFor(m => m.Show, new { @class = "switchcheckbox" })</td>
<td>@Html.CheckBoxFor(m => m.ShowExternal, new { @class = "switchcheckbox" })</td>
<td>
    @Html.TextBoxFor(m => m.Label)
    <br />
    @Html.ValidationMessageFor(m => m.Label)
</td>
<td>@Html.CheckBoxFor(m => m.Required, new { @class = "switchcheckbox" })</td>
<td>@Html.CheckBoxFor(m => m.ShowInList, new {@class = "switchcheckbox"})</td>
<td>@Html.CheckBoxFor(m => m.IsMultiple, new { @class = "switchcheckbox" })</td>
<td>
    @Html.TextBoxFor(m => m.DefaultValue)
    <br />
    @Html.ValidationMessageFor(m => m.DefaultValue)
</td>
<td>@Html.TextBoxFor(m => m.Help)</td>