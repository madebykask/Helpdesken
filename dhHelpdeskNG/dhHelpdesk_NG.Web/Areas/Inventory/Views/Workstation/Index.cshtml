@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Areas.Inventory.Models.SearchModels.WorkstationSearchViewModel
@{
    Layout = "~/Areas/Inventory/Views/Shared/_IndexInventoryLayout.cshtml";
}

@section Actions
{
    <ul class="secnav">
        @if (Model.UserHasInventoryAdminPermission)
        {
            <li>
                @Html.ActionLink(Translation.Get("Ny arbetsstation"), "New", null, new { @class = "btn" })
            </li>
        }
        <li>
            @Html.ActionLink(Translation.Get("Excel"), "ExportGridToExcelFile", null, new { @class = "btn", id = "excel_report_inventory_button" })
        </li>
    </ul>
}

@Html.Partial("Workstations", Model)