@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Areas.Inventory.Models.EditModel.Computer.OtherFieldsModel
@if (Model.Info.Show)
{
    <h4>@Translation.Get("Övrigt")</h4>
    <div class="row">
        <div class="span12">
            <table class="tableform">
                @Html.EditorFor(m => m.Info, "Fields/WideText")
            </table>
        </div>
    </div>
}
else
{
    @Html.HiddenFor(m => m.Info.Value)
}
