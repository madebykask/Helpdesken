@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Areas.Inventory.Models.EditModel.Computer.GraphicsFieldsModel
@if (Model.VideoCard.Show)
{
    <h4>@Translation.Get("Grafik")</h4>
    <div class="row">
        <div class="span12">
            <table class="tableform">
                @Html.EditorFor(m => m.VideoCard, "Fields/Text")
            </table>
        </div>
    </div>
}
else
{
    @Html.HiddenFor(m => m.VideoCard.Value)
}
