@using DH.Helpdesk.Web.Infrastructure
@model List<string>
<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>@Translation.Get("Program")
            </th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var model in Model)
            {
            <tr>
                <th>@Html.DisplayFor(x => model);
                </th>
            </tr>
            }
        }
    </tbody>
</table>
