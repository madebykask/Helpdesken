@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Areas.Inventory.Models.EditModel.Settings.Computer.ComputerFieldsSettingsViewModel
<script type="text/javascript">
    $(function () {
        var parameters = {
            saveSettingsUrl: '@Url.Action("WorkstationSettings")',
            settingsSavedSuccessfullyMessage: '@Translation.GetForJS("Inställningarna sparades")'
        };

        applySettingsBehavior(parameters);
    });
</script>
@using (Ajax.BeginForm("WorkstationSettings", "InventorySettings", new AjaxOptions { OnSuccess = "onSettingsSavedSuccessfully", HttpMethod = "POST" }, new { id = "settings_form" }))
{
    @Html.EditorFor(m => m, "Settings/ComputerFieldsSettingsViewModel")
}