@using System.Web.Optimization
@using DH.Helpdesk.Web.Infrastructure
@model int
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render("~/bundles/changes/settings")
@Scripts.Render("~/bundles/inventory/settings")

<div class="secnavs fixed">
    <div class="pull-left">
        <ul class="secnav">
            <li id="menu1">
                <input type="submit" value="@Translation.Get("Spara")" class="btn" onclick=" $('#settings_form').submit(); "/>
            </li>
            <li id="menu2">@Html.ActionLink(Translation.Get("Avbryt"), "Index", null, new { @class = "btn" })</li>
            <li id="menu3">@Html.ActionLink(Translation.Get("Ta bort"), "DeleteInventoryType",  new { inventoryTypeId = Model }, new { @class = "btn" })</li>
        </ul>
    </div>
</div>
<div class="content">
    <div class="container">
        <div id="settings_container">
            @Html.Action("InventorySettings", "InventorySettings", new { inventoryTypeId = Model })
        </div>
    </div>
</div>
