@model DH.Helpdesk.Web.Areas.Inventory.Models.EditModel.Settings.Computer.FieldSettingModel
<td>@Html.LabelFor(m => m)</td>
<td>@Html.CheckBoxFor(m => m.ShowInDetails, new { @class = "switchcheckbox" })</td>
<td>@Html.TextBoxFor(m => m.Caption, Model.IsCaptionDisabled ? (object)new { @disabled = "disabled" } : (object)new { })</td>
<td>
    @Html.CheckBoxFor(m => m.IsRequired, Model.IsRequiredDisabled ? (object)new { @class = "is_required_checkbox switchcheckbox", @disabled = "disabled" } : (object)new { @class = "is_required_checkbox switchcheckbox" })
</td>
<td>@Html.CheckBoxFor(m => m.ShowInList, Model.IsShowInListDisabled ? (object)new { @class = "switchcheckbox", @disabled = "disabled" } : (object)new { @class = "switchcheckbox" })</td>
<td>
    @Html.CheckBoxFor(m => m.IsReadOnly, Model.IsReadOnlyDisabled ? (object)new { @class = "is_readonly_checkbox switchcheckbox", @disabled = "disabled" } : (object)new { @class = "is_readonly_checkbox switchcheckbox" })
</td>

