@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Areas.Inventory.Models.EditModel.Server.OtherFieldsModel
@if (Model.Info.Show
        || Model.Other.Show
        || Model.URL.Show
        || Model.URL2.Show
        || Model.Owner.Show)
{
    <h4>@Translation.Get("Övrigt")</h4>
    <div class="row">
        <div class="span12">
            <table class="tableform">
                @Html.EditorFor(m => m.Info, "NotReadonlyFields/WideText")
                @Html.EditorFor(m => m.Other, "NotReadonlyFields/Text")
                @Html.EditorFor(m => m.URL, "NotReadonlyFields/Text")
                @Html.EditorFor(m => m.URL2, "NotReadonlyFields/Text")
                @Html.EditorFor(m => m.Owner, "NotReadonlyFields/Text")
            </table>
        </div>
    </div>
}

