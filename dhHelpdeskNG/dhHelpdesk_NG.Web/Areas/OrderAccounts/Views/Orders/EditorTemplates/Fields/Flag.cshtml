@using DH.Helpdesk.Web.Infrastructure.Extensions.HtmlHelperExtensions
@model DH.Helpdesk.Web.Areas.OrderAccounts.Models.Order.FieldModels.ConfigurableFieldModel<bool>
@if (!Model.Show)
{
    return;
}

<tr>
    <td class="span3">
        @Html.FieldCaption(Model)
    </td>
    <td class="span9">
        @Html.CheckBoxFor(m => m.Value, new { @disabled = "disabled" })
    </td>
</tr>
