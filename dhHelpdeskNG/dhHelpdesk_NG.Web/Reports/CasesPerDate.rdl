  <DataSets>
    <DataSet Name="CasesPerDate">
      <Query>
        <DataSourceName>Helpdesk_local</DataSourceName>
        <CommandText>SELECT        COUNT(tblCase.Casenumber) AS Volume, tblCustomer.Name, tblDate.DateKey, tblCustomer.Id
FROM            tblCustomer INNER JOIN
                         tblCase ON tblCustomer.Id = tblCase.Customer_Id RIGHT OUTER JOIN
                         tblDate ON CAST(tblCase.RegTime AS Date) = tblDate.FullDate
GROUP BY tblDate.DateKey, tblCustomer.Name, tblCustomer.Id
ORDER BY tblDate.DateKey</CommandText>
      </Query>
      <Fields>
        <Field Name="Volume">
          <DataField>Volume</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DateKey">
          <DataField>DateKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CustomerName">
      <Query>
        <DataSourceName>Helpdesk_local</DataSourceName>
        <CommandText>SELECT        Name, Id
FROM            tblCustomer</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
