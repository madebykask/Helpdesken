@model dhHelpdesk_NG.Domain.CaseSolutionCategory
@{
    ViewBag.Title = "NewCategory";
}
@using (Html.BeginForm("newcategory", "caseSolution", new { id = Model.Id }, FormMethod.Post, new { id = "target" }))
{
    <div class="secnavs">
        <div class="pull-left">
            <ul class="secnav">
                <li>@Html.ActionLink(Translation.Get("Spara", Enums.TranslationSource.TextTranslation), "new", null, new { onclick = "$('#target').submit();return false;", @class = "btn" })</li>
                <li>@Html.ActionLink(Translation.Get("Avbryt", Enums.TranslationSource.TextTranslation), "index", null, new { @class = "btn" })</li>
            </ul>
        </div>
    </div>
    @Html.Partial("_InputCategory", Model)
}
<script type="text/javascript">
    $("input:text:visible:first").focus();
</script>
