@model DH.Helpdesk.Web.Models.OperationLogInputViewModel
@{
    ViewBag.Title = "";
}

@using (Html.BeginForm("new", "operationlog", new { id = Model.OperationLog.Id }, FormMethod.Post, new { id = "target" }))
{
    HtmlHelper.UnobtrusiveJavaScriptEnabled = false;
    HtmlHelper.ClientValidationEnabled = false;

    <div class="secnavs fixed">
      <div class="pull-left">
        <ul class="secnav">    
          <li>@Html.ActionLink(Translation.GetCoreTextTranslation("Spara"), "new", null, new { onclick = "submitForm();return false;" , @class = "btn" })</li>
          <li> @Html.ActionLink(Translation.GetCoreTextTranslation("Avbryt"), "index",null,new { @class = "btn" }) </li>
        </ul>      
      </div>
    </div>
    
    <div class="contentContent">
        @Html.Partial("_Input", Model)
    </div>
}

