@model IList<dhHelpdesk_NG.Web.Models.Problem.Output.LogOutputModel>
<table id="tblCases" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>@Translation.Get("Date", Enums.TranslationSource.TextTranslation)
            </th>
            <th>@Translation.Get("Registrated By", Enums.TranslationSource.TextTranslation)
            </th>
            <th>@Translation.Get("Log Note", Enums.TranslationSource.TextTranslation)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr onclick="window.location.href = '@this.Url.Action("Problem", new RouteValueDictionary { { "id", item.Id } })'">
                <td>@Html.DisplayFor(x => item.Date)
                </td>
                <td>@Html.DisplayFor(x => item.RegistratedBy)
                </td>
                <td>@Html.DisplayFor(x => item.LogNote)
                </td>
            </tr>
        }
    </tbody>
</table>
