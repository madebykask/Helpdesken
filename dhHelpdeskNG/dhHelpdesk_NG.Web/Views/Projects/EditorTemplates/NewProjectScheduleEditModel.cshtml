@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Models.Projects.NewProjectScheduleEditModel
<div class="container">
    <h4>@Translation.Get("Nytt delprojekt", Enums.TranslationSource.TextTranslation)</h4>
    <div class="row">
        <div class="span12">
            <table>
                <tr>
                    <td style="padding-right:10px;">@Html.LabelFor(x => x.ProjectScheduleEditModel.Position)
                    </td>
                    <td style="padding-right:20px;">@Html.DropDownListFor(x => x.ProjectScheduleEditModel.Position, new SelectList(Model.Positions, "Value", "Text", Model.ProjectScheduleEditModel.Position), new { @class="inputw50" })
                    </td>
                    <td style="padding-right:10px;">@Html.LabelFor(x => x.ProjectScheduleEditModel.Name)
                    </td>
                    <td style="padding-right:10px;">@Html.EditorFor(x => x.ProjectScheduleEditModel.Name)
                    </td>
                    <td><input type="submit" value="@Translation.Get("Nytt delprojekt")" class="btn"/></td>
                </tr>
            </table>
        </div>
    </div>
</div>
@Html.HiddenFor(x => x.ProjectScheduleEditModel.Id)
@Html.HiddenFor(x => x.ProjectScheduleEditModel.ProjectId)
