@model DH.Helpdesk.Web.Models.Case.CaseOperationResult
@{
    ViewBag.Title = "";
 }

<div class="content nonemenu">
    <div class="container">
        <ul class="nav nav-tabs">
            <li data-active-tab="case-tab" class="active"><a href="#">@Translation.Get("Ärende") @Model.CaseNumber</a></li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane tab-pane-border active">
                <div class="container">
                    <div class="@(Model.Success ? "dh-success" : "dh-error")">@Html.Raw(Model.Message)</div>
                </div>
            </div>
        </div>
    </div>
</div>
