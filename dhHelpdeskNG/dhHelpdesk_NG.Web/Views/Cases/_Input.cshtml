    function FieldIsRequired(value) {
        var ret = false;
        if (value == '1') {
            ret = true;
        }
        return ret;
    }

    $(function() {
        $("#caseDescriptionPreview").click(function() {
            var description = $(".case-description")
                .attr("value")
                .replace(/\r\n|\r|\n/g, "<br />");
            var d = $(document.createElement("div"))
                .html(description)
                .dialog({
                    title: "@Translation.Get(GlobalEnums.TranslationCaseFields.Description.ToString(), Enums.TranslationSource.CaseTranslation, Model.case_.Customer_Id)",    
                    modal: true,
                    width: 600,
                    height: 400,
                    close: function() {
                        d.dialog("destroy");
                    }
                });
        });

        $("[data-case-field-description]").click(function () {
            var $this = $(this);
            var caption = $this.attr("data-case-field-caption");
            var description = $this.attr("data-case-field-description");
            var d = $(document.createElement("div"))
                .html(description)
                .dialog({
                    title: caption,
                    modal: true,
                    close: function () {
                        d.dialog("destroy");
                    }
                });
        });
    });

</script>


