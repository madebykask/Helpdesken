                                                    <a href="#" id="AddNotifier" class="btn">@Translation.Get("Lägg till")</a> 
                                                </td>
                                            </tr>
                                            <tr style="@Model.caseFieldSettings.displayHtml(GlobalEnums.TranslationCaseFields.Persons_Name.ToString())">
                                                <td class="span3">
                                                    @CaseFieldCaption(GlobalEnums.TranslationCaseFields.Persons_Name)
                                                </td>
