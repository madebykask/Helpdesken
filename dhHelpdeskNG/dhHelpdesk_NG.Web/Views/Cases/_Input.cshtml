                                                            {
                                                                @:<input type="text" class="inputw100" id="ExternalInvoices[@extInvoicesCount].Amount" name="ExternalInvoices[@extInvoicesCount].Amount" value="@externalInvoice.AmountValue" placeholder="0.00" disabled="disabled">
                                                            }
