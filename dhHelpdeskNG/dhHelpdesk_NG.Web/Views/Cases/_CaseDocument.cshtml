@model IEnumerable<DH.Helpdesk.BusinessData.Models.CaseDocument.CaseDocumentModel>
@using DH.Helpdesk.Common.Constants;

@if (Model != null && Model.Any())
{



    <h4>@(Translation.GetCoreTextTranslation("Case Documents"))</h4>
            @*<p> Förtydliga: Dynamiska dokument baseras på senast sparade värden.</p>*@

            <table class="table table-striped table-full-width">
                <thead>
                    <tr>
                        <th class="span4">Name</th>
                        <th class="span4"></th>
                        <th class="span4"></th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @item.Name<text>.</text>@item.FileType
                    </td>
                    <td>
                        @*ska hämta baserat på caseGuid?*@
                        @*<a href="/casedocument/CaseDocumentGet?caseDocumentGUID=@item.CaseDocumentGUID" target="_blank" title="@Translation.GetCoreTextTranslation("Skriv ut") @item.Name" class="btn print-case"><i class="icon-file"></i> @Translation.GetCoreTextTranslation("Skriv ut")</a>*@

                        <a href="/casedocument/CaseDocumentGet?caseDocumentId=@item.Id&caseId=@item.CaseId" target="_blank" title="@Translation.GetCoreTextTranslation("Skriv ut") @item.Name" class="btn print-case"><i class="icon-file"></i> @Translation.GetCoreTextTranslation("Skriv ut")</a>
                    </td>
                    <td></td>
                </tr>
                }


            </table>





}