
@model DH.Helpdesk.Web.Models.Changes.ChangeEdit.HistoryModel
           
<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                @Html.LabelFor(m => m.HistoryRecords.First().DateAndTime)
            </th>
            <th>
                @Html.LabelFor(m => m.HistoryRecords.First().RegisteredBy)
            </th>
            <th>
                @Html.LabelFor(m => m.HistoryRecords.First().Log)
            </th>
            <th>
                @Html.LabelFor(m => m.HistoryRecords.First().FieldsDifferences)
            </th>
            <th>
                @Html.LabelFor(m => m.HistoryRecords.First().Emails)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var historyRecord in Model.HistoryRecords)
        {
            @Html.DisplayFor(m => historyRecord)
        }
    </tbody>
</table>