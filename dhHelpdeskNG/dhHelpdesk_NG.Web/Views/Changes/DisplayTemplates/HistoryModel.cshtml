@model dhHelpdesk_NG.Web.Models.Changes.HistoryModel
           
<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                @Html.LabelFor(m => m.Histories.First().DateAndTime)
            </th>
            <th>
                @Html.LabelFor(m => m.Histories.First().RegisteredBy)
            </th>
            <th>
                @Html.LabelFor(m => m.Histories.First().Log)
            </th>
            <th>
                @Html.LabelFor(m => m.Histories.First().History)
            </th>
            <th>
                @Html.LabelFor(m => m.Histories.First().Emails)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var historyItem in Model.Histories)
        {
            @Html.DisplayFor(m => historyItem)
        }
    </tbody>
</table>