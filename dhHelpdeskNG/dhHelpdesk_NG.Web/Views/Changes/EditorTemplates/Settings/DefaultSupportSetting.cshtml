
@model DH.Helpdesk.Web.Models.Changes.SettingsEdit.TextFieldSettingModel

<td>@Html.LabelFor(m => m)</td>
<td>@Html.CheckBoxFor(m => m.ShowInDetails, new { @class = "switchcheckbox" })</td>
<td>@Html.CheckBoxFor(m => m.ShowInChanges, new { @class = "switchcheckbox" })</td>
<td>@Html.CheckBoxFor(m => m.ShowInSelfService, new { @class = "switchcheckbox" })</td>
<td>@Html.TextBoxFor(m => m.Caption, new { @style = "max-width:90%;" })</td>
<td>@Html.CheckBoxFor(m => m.Required, new { @class = "switchcheckbox" })</td>
<td>@Html.TextBoxFor(m => m.DefaultValue, new { @style = "max-width:90%;" })</td>
<td>@Html.TextBoxFor(m => m.Bookmark, new { @style = "max-width:90%;" })</td>