
@model DH.Helpdesk.Web.Models.Changes.ChangeEdit.ConfigurableFieldModel<string>
           
@if (!Model.Show)
{
    return;
}           

@Html.EditorFor(m => m.Value, "TimePicker", new { id = ViewData["Id"], inputAttributes = new Dictionary<string, object> { { "data-dh-is-required", Model.IsRequired } } })
@Html.HiddenFor(m => m.IsRequired)
@Html.ValidationMessageFor(m => m.Value)