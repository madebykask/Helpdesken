<script>
    $(function () {
        $(".is_required").change(function () {
            var value = $(this).is(":checked");
            $(this).closest("tr").find(".is_readonly").prop('checked', !value);
        });

        $(".is_readonly").change(function () {
            var value = $(this).is(":checked");
            $(this).closest("tr").find(".is_required").prop('checked', !value);
        });
    });
</script>
