@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Models.BulletinBoardInputViewModel
@{
    ViewBag.Title = "New";
}
@using (Html.BeginForm("new", "bulletinboard", new { id = Model.BulletinBoard.Id }, FormMethod.Post, new { id = "target" }))
{
    <div class="secnavs fixed">
        <div class="pull-left">
            <ul class="secnav">
                <li>@Html.ActionLink(Translation.Get("Spara", Enums.TranslationSource.TextTranslation), "new", null, new { @class = "btn save" })</li>
                <li>@Html.ActionLink(Translation.Get("Avbryt", Enums.TranslationSource.TextTranslation), "index", null, new { @class = "btn" })</li>
            </ul>
        </div>
    </div>
    @Html.Partial("_Input", Model)
}