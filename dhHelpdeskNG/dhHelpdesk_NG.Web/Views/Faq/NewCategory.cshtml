@using dhHelpdesk_NG.Web.Models.Faq.Output

@model NewCategoryModel

@{
    this.ViewBag.Title = @Translation.Get("New FAQ Category", Enums.TranslationSource.TextTranslation);
}

<form id="category_edit_form" action="@this.Url.Action("NewCategory")" method="POST">
    <input type="hidden" name="ParentCategoryId" value="@this.Model.ParentCategoryId" />
    <div class="secnavs">
        <div class="pull-left">
            <ul class="secnav">
                <li><input type="submit" value="@Translation.Get("Save", Enums.TranslationSource.TextTranslation)" class="btn"/></li>
                <li><a href="@this.Url.Action("Index")" class="btn">@Translation.Get("Cancel", Enums.TranslationSource.TextTranslation)</a></li>
            </ul>
        </div>
    </div>
    @this.Html.Partial("CategoryEditing", new EditingCategoryModel(string.Empty))
</form>