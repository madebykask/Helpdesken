@model IList<DH.Helpdesk.BusinessData.Models.Case.ExtendedCaseFormModel>
@using DH.Helpdesk.Web;

@if (Model != null && Model.Any())
{
        @Scripts.Render(BundleConfig.ScriptNames.DynamicCase)
        <script type="text/javascript">

        @foreach (var extendedCase in Model)
        {
            
              extendedCase.Path = extendedCase.Path.Replace("\\", "");
        <text>
        $(function () {
            var urlToLoad = '@extendedCase.Path'.replace(/&amp;/g,'&');

            new dhform({ url: urlToLoad, modal: '0', formAreaId: 'container_@extendedCase.Id' });
            });
        </text>
        }
        </script>
    }

