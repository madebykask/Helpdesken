@model DH.Helpdesk.BusinessData.Models.Statistics.Output.StatisticsOverview

@if (Model != null)
{
    <div class="row-fluid">
        <div class="box-box span12">
            <div class="box-header portlet-header">
                <div class="start-tile-img label green">
                    <img src="@Url.Content("~/Content/icons/chart.png")"
                         alt="@Translation.Get("Statistik")" />
                </div><h4>@Translation.Get("Statistik")</h4>
            </div>
            <div class="box-body portlet-content">
                <div class="box-post">
                    <div id="statisticsData">
                        <table class="table-condensed">
                            <tr sty>
                                <td>@Translation.Get("Pågående")</td>
                                <td>@Translation.Get("Oöppnad")</td>
                                <td>@Translation.Get("Vilande")</td>
                                <td>@Translation.Get("Försenad")</td>
                            </tr>
                            <tr>
                                <td id="st_inProgress"><i class="gif-loader" style="">&nbsp;</i></td>
                                <td id="st_unopend"><i class="gif-loader" style="">&nbsp;</i></td>
                                <td id="st_onhold"><i class="gif-loader" style="">&nbsp;</i></td>
                                <td id="st_delayed"><i class="gif-loader" style="">&nbsp;</i></td>
                            </tr>
                            <tr>
                                <td>@Translation.Get("Nya idag")</td>
                                <td>@Translation.Get("Försenad idag")</td>
                                <td>@Translation.Get("Löst idag")</td>
                                <td>@Translation.Get("Löst i tid idag")</td>
                            </tr>
                            <tr>
                                <td id="st_newToday"><i class="gif-loader" style="">&nbsp;</i></td>
                                <td id="st_delayedToday"><i class="gif-loader" style="">&nbsp;</i></td>
                                <td id="st_solved"><i class="gif-loader" style="">&nbsp;</i></td>
                                <td><span id="st_solvedInTime"><i class="gif-loader" style="">&nbsp;</i></span>%</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
