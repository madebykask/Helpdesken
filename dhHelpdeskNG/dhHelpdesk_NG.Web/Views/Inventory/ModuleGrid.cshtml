@model DH.Helpdesk.Web.Models.Inventory.EditModel.ComputerModuleGridModel
@if (Model.Overviews.Any())
{
    <table id="tblCases" class="table table-striped table-bordered table-hover">
        <tbody>
            @foreach (var item in Model.Overviews)
            {
                <tr onclick=" window.location.href = '@Url.Action("EditComputerModule", new { id = Convert.ToInt32(item.Value), name = item.Name, currentMode = Model.ModuleType })' ">
                    <td class="tdpointer">@Html.DisplayFor(x => item.Name)
                    </td>
                </tr>
            }
        </tbody>
    </table>
}


