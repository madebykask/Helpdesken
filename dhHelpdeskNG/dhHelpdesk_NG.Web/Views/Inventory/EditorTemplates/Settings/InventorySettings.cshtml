@model DH.Helpdesk.Web.Models.Inventory.EditModel.Settings.Inventory.InventoryFieldSettingsViewModel
<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>@Translation.Get("Grupp", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Egenskap", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Pos", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Typ", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Storlek", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Visa", Enums.TranslationSource.TextTranslation)</th>
            <th>@Translation.Get("Lista", Enums.TranslationSource.TextTranslation)</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td colspan="7" class="header">@Translation.Get("Allmänt", Enums.TranslationSource.TextTranslation)</td>
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.DepartmentFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.NameFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.ModelFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.ManufacturerFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.SerialNumberFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.TheftMarkFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            <td colspan="7" class="header">@Translation.Get("Inventering", Enums.TranslationSource.TextTranslation)</td>
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.BarCodeFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.PurchaseDateFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            <td colspan="7" class="header">@Translation.Get("Placering", Enums.TranslationSource.TextTranslation)</td>
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.PlaceFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            <td colspan="7" class="header">@Translation.Get("Övrigt", Enums.TranslationSource.TextTranslation)</td>
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.InfoFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            <td colspan="7" class="header">@Translation.Get("Tillbehör till", Enums.TranslationSource.TextTranslation)</td>
        </tr>
        <tr>
            @Html.EditorFor(m => m.DefaultSettings.WorkstationFieldSettingModel, "Settings/InventoryFieldSettingModelDefault")
        </tr>
        <tr>
            <td colspan="7" class="header">@Translation.Get("Additional", Enums.TranslationSource.TextTranslation)</td>
        </tr>
        @{
            var fields = Model.InventoryDynamicFieldSettings.OrderBy(x => x.Position).ThenBy(x => x.Caption);

            foreach (var item in fields)
            {
            <tr>
                @Html.EditorFor(m => item, "Settings/InventoryFieldSettingModel")
            </tr>
            }
        }
    </tbody>
</table>
