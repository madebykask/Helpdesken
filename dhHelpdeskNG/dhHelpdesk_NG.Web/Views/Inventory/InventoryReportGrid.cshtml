@model List<DH.Helpdesk.BusinessData.Models.Inventory.InventoryReportModel>
<table id="tblCases" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>@Translation.Get("Inventarie")
            </th>
            <th>@Translation.Get("Antal")
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(x => item.InventoryName)
                </td>
                <td>@Html.DisplayFor(x => item.Count)
                </td>
            </tr>
        }
    </tbody>
</table>

