@using DH.Helpdesk.Web.Infrastructure
@model DH.Helpdesk.Web.Models.DocumentInputViewModel
@{
    ViewBag.Title = "New";
}
@using (Html.BeginForm("new", "document", new { id = Model.Document.Id }, FormMethod.Post, new { id = "target", enctype = "multipart/form-data" }))
{
    <div class="secnavs">
        <div class="pull-left">
            <ul class="secnav">
                <li>@Html.ActionLink(Translation.Get("Spara", Enums.TranslationSource.TextTranslation), "new", null, new { onclick = "submitForm();return false;", @class = "btn" })</li>
                <li>@Html.ActionLink(Translation.Get("Avbryt", Enums.TranslationSource.TextTranslation), "index", null, new { @class = "btn" })</li>
            </ul>
        </div>
    </div>
    @Html.Partial("_Input", Model)
}