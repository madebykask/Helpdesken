<div class="content">
    <div class="container">
        @if (ViewBag.CaseNumber != null)
        {
            <ul class="nav nav-tabs"><li class="active"><a href="#fragment=1">@Translation.Get("Ärende") @ViewBag.CaseNumber</a></li></ul>
        }
        <div class="tab-content">
            <div class="tab-pane tab-pane-border active">
                <div class="container">
                    <h2>@Translation.Get("Error")</h2>
                    <div>@Translation.Get("Du kan inte fylla i utvärderingen på grund av")<span>&nbsp;@Translation.Get(ViewBag.ErrorReason)</span></div>
                </div>
            </div>
        </div>
    </div>
</div>