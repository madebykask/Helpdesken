@model ECT.FormLib.Models.FormModel
@using ECT.FormLib;
@using ECT.FormLib.Models;

@{
    ViewBag.Title = Model.Translate("Poland") + " - " + Model.Translate("Mandatory agreements");
}

@section Script {
 <script>

     validate.run();

     $(document).on('change', '#actionState', function (e) {
         var val = $(this).val();
         validate.run(val);
     });

    </script>

    @Html.Partial("_Validate", Model)
}

@Html.Partial("_Edit", Model)

