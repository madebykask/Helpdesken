		<add key="dh_filesDirectory" value="#{FilesDirectory}" />
		<add key="dh_selfserviceaddress" value="#{SelfServiceAddress}" />
		<add key="dh_helpdeskaddress" value="#{HelpdeskAddress}" />
		<add key="DefaultCulture" value="sv" />
		<add key="SmtpServer" value="smtp.datahalland.se" />
		<add key="SmtpPort" value="25" />
		<add key="logRequest" value="false" />
		<add key="MicrosoftClientId" value="#{MicrosoftClientIdApi}"/>
		<add key="MicrosoftTenant" value="#{MicrosoftTenantApi}"/>
		<add key="MicrosoftAuthority" value="#{MicrosoftAuthorityApi}"/>
		<add key="WebpartSecretKey" value="#{WebpartSecretKey}"/>
		<add key="WebpartSecretKey" value="#{WebpartAcceptedCustomerIds}"/>
	</appSettings>

	<connectionStrings>
