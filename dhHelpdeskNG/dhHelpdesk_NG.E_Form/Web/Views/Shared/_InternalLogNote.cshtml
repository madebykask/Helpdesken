@model FormModel

@if(Model.Contract.StateSecondaryId <= 10 
    && (Model.Contract.CaseHistory == null || !Model.Contract.CaseHistory.Any()))
{
    <tr>
        <td class="tdstrong" colspan="2">
            <h4>@Model.Label("InternalLogNote")</h4>
        </td>
    </tr>
    <tr>
        <td colspan="2">
            @Model.Element("InternalLogNote", "span12")
        </td>
    </tr>
}