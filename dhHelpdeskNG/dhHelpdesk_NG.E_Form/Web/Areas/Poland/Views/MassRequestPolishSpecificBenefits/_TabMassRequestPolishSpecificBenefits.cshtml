
@model FormModel

<div class="tab-pane active" id="MassRequestPolishSpecificBenefits">
    <div class="row-fluid">
        <div class="span12">
            <table class="table">
                <tr>
                    <td class="tdstrong">@Model.Translate("Initiator")</td>
                    <td>
                        <input type="text" value="@Model.Contract.Initiator" readonly="readonly"/></td>
                </tr>
                <tr>
                    <td class="tdstrong">@Model.Label("Unit")</td>
                    <td>
                        @Model.Element("Unit")
                    </td>
                </tr>
                <tr>
                    <td class="tdstrong">@Model.Label("NumberOfRecords")</td>
                    <td>
                        @Model.Element("NumberOfRecords")
                    </td>
                </tr>
                @Html.Partial("_Attachments", Model)
                @Html.Partial("_InternalLogNote", Model)
            </table>
        </div>
    </div>
</div>
