@model DH.Helpdesk.EForm.Service.Form

@{
    ViewBag.Title = "Edit";
}

<div id="wrapper">

    <!-- Sidebar -->
    <div id="sidebar-wrapper">
        <ul class="sidebar-nav">
            <li class="sidebar-brand">
                <a href="#">&nbsp;
                </a>
            </li>
            @foreach(var tab in Model.Tabs)
            {
                <li><a href="#" class=""><i class="fa fa-file"></i>&nbsp;@tab.Name</a>
                    <ul>
                        @foreach(var element in tab.Elements)
                        {
                            <li><a href="#">@element.Name</a></li>
                        }
                    </ul>
                </li>
            }
        </ul>
    </div>
    <!-- /#sidebar-wrapper -->

    <!-- Page Content -->
    <div id="page-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    
                </div>
            </div>
        </div>
    </div>
    <!-- /#page-content-wrapper -->

</div>
<!-- /#wrapper -->


